<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	<record model='ir.ui.view' id='rbs_documento_certificaciones'>
    		<field  name="name">rbs.documento.certificaciones</field>
    		<field  name="model">res.company</field>
    		<field  name="inherit_id" ref="report.reporting_settings_form_inherited"/>
			<field name='type'>form</field>    		
    		<field name="arch" type="xml">
				<xpath expr="//page[@string='General Information']" position="after">
                    <page string="Certificaciones">
					<group>
						<field name="certificacion"></field>
						<field name="inscripcion"></field>
					</group>
				</page>
                </xpath>

                
    		</field>

    	</record>    
    </data>
</openerp>
	