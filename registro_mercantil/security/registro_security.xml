<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.module.category" id="module_category_registro_mercantil">
            <field name="name">Registro Mercantil y de Propiedad</field>
            <field name="description"></field>
            <field name="sequence">6</field>
        </record>

        <record id="group_certificador" model="res.groups">
            <field name="name">Certificador</field>
            <field name="category_id" ref="module_category_registro_mercantil"/>
            <field name="comment">Usuario que crea certificaciones y las firma</field>
        </record>

        <record id="group_registrador" model="res.groups">
            <field name="name">Registrador</field>
            <field name="category_id" ref="module_category_registro_mercantil"/>
            <field name="comment">Usuario que registra los movimientos</field>
        </record>

        <record id="group_cajero" model="res.groups">
            <field name="name">Cajero</field>
            <field name="category_id" ref="module_category_registro_mercantil"/>
            <field name="implied_ids" eval="[(4, ref('base.group_sale_manager'))]"/>
            <field name="comment">Usuario que registra factura y crea tareas</field>
        </record>

        <record id="group_administrador" model="res.groups">
            <field name="name">Administrador</field>
            <field name="category_id" ref="module_category_registro_mercantil"/>
        </record>

    </data>
</openerp>
