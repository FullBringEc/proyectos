<t t-name="pdf_binary">
    <span class="o_form_field_image">
        <t t-if="!widget.get('effective_readonly')">
            <div class="o_form_image_controls">
                <span class="fa fa-pencil fa-lg pull-left o_select_file_button" title="Edit"/>
                <span class="fa fa-trash-o fa-lg pull-right o_clear_file_button" title="Clear"/>

                <span class="o_form_binary_progress">Uploading...</span>
                <t t-call="HiddenInputFile">
                    <t t-set="fileupload_id" t-value="widget.fileupload_id"/>
                </t>
            </div>
        </t>
        <br/>
        <div class="divgeneral">
            <div class="contenido" style="text-align: center">
                


                <table class="table">
                     
                 <td style="text-align: center">
                    <span class="button buttonback btn" >
                        <span>Atras</span>
                    </span>

                    <span class="button buttonnext btn">
                        <span>Adelante</span>
                    </span>
                </td>
                <td>
                    <span class="input ">
                    <input class="input__field pagina_actual" type="text" />
                    </span>
                    /
                    <span class="input ">
                        <input class="input__field pagina_final" type="text" readonly="True" />
                    </span>
                </td>    
                </table>
                 
            </div>
            
            <div class="divC">
                <canvas class="imagenCanvas"> </canvas>
            </div>

            <div class="contenido">
                <button class="button exportar_pdf" style="vertical-align:middle">
                    <span><i class="fa fa-print" aria-hidden="true"></i></span>
                </button>
            </div>
        </div>
    </span>
</t>


